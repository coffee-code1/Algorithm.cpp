#include<bits/stdc++.h>
using namespace std;
bool cmp(string s1,string s2){
    return (s1+s2)>(s2+s1);//字典序规则，根据前一个字符的ascll值来判断，谁大谁就大，否则下一位字符一直到最后，若长度不一，前面都相等则短的那个大比如3和32，2>32，这里就将两个拼接，使得长度一样,返回拼接方式大的一种。
}
int main(){
    int n;
    string s[21];
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>s[i];
    }
    sort(s,s+n,cmp);
    for(int i=0;i<n;i++){
        cout<<s[i];
    }
}
